; VHDL 2008 pretty operators
(signal_assignment "<=" @operator (#set! conceal "⇐"))
(variable_assignment ":=" @operator (#set! conceal "≔"))
(element_association "=>" @operator (#set! conceal "⇒"))
(relational_operator "<=" @operator (#set! conceal "≤"))
(relational_operator ">=" @operator (#set! conceal "≥"))
(relational_operator "/=" @operator (#set! conceal "≠"))

